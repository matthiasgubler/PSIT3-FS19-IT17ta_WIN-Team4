            </cellValueFactory>
         </TableColumn>
         <TableColumn editable="false" prefWidth="450.0" text="Name Skill">
            <cellValueFactory>
               <PropertyValueFactory property="name" />
            </cellValueFactory>
         </TableColumn>
         <TableColumn editable="false" prefWidth="75.0" text="Anz. MA">
            <cellValueFactory>
               <PropertyValueFactory property="count" />
            </cellValueFactory>
         </TableColumn>
      </columns>
      <items>
         <FXCollections fx:id="developmentSkillUIObservableList" fx:factory="observableArrayList" />
      </items>
   </TableView>
   <Label layoutX="14.0" layoutY="14.0" text="Liste der Skills fÃ¼r einen Workshop">
