<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.collections.FXCollections?>
<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zhaw.ch.skill7.view.admin.AdminDevelopmentTableController">
   <TableView fx:id="tableView" editable="true" prefHeight="400.0" prefWidth="600.0">
      <columns>
         <TableColumn editable="false" prefWidth="75.0" text="ID">
            <cellValueFactory>
               <PropertyValueFactory property="id"/>
            </cellValueFactory>
         </TableColumn>
         <TableColumn prefWidth="525.0" text="Name des Skills">
            <cellValueFactory>
               <PropertyValueFactory property="name"/>
            </cellValueFactory>
         </TableColumn>
      </columns>
      <items>
         <FXCollections fx:id="skillUIObservableList" fx:factory="observableArrayList"/>
      </items>
   </TableView>
</AnchorPane>
