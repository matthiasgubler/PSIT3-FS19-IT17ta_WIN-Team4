<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="353.0" prefWidth="530.0" type="AnchorPane"
         xmlns="http://javafx.com/javafx/8.0.172-ea">
    <children>
        <BarChart fx:id="barChart" animated="false" barGap="1.0" categoryGap="1.0" layoutX="14.0" layoutY="12.0"
                  legendVisible="false" prefHeight="330.0" prefWidth="440.0" title="TITLE">
            <xAxis>
                <CategoryAxis side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT"/>
            </yAxis>
        </BarChart>
        <Button fx:id="reloadButton" layoutX="455.0" layoutY="24.0" mnemonicParsing="false" text="Reload"
                visible="reloadButtonVisible"/>
    </children>
</fx:root>
