<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea"
            fx:controller="com.zhaw.ch.skill7.view.skill.SkillCreationController">
    <GridPane prefHeight="347.0" prefWidth="600.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="522.0" minWidth="10.0" prefWidth="431.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="169.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="170.0" minHeight="10.0" prefHeight="34.0" vgrow="SOMETIMES"/>
            <RowConstraints fillHeight="false" maxHeight="313.0" minHeight="10.0" prefHeight="313.0" valignment="TOP"
                            vgrow="SOMETIMES"/>
        </rowConstraints>
        <children>
            <Label text="Titel"/>
            <TextField fx:id="newSkillTextField" GridPane.rowIndex="1"/>
            <Button mnemonicParsing="false" onAction="#creationButtonClicked" prefHeight="27.0" prefWidth="146.0"
                    text="Erfassen" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        </children>
    </GridPane>
</AnchorPane>
